%     Boxed Equations
\newlength{\lllen}
\newcommand{\boxeq}[1]{
  \setlength{\lllen}{\textwidth}
  \addtolength{\lllen}{-2\fboxsep}
  \addtolength{\lllen}{-2\fboxrule}
  \noindent
  \fbox{%
  \begin{minipage}{\lllen}
  \vspace{-\abovedisplayskip}
  {#1}\end{minipage}}}
